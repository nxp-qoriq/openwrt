-IFC_GRP_A_EXT=1
-IFC_GRP_D_BASE=1
-IFC_GRP_D_EXT=1
-IFC_GRP_E1_BASE=1
-IFC_GRP_E1_EXT=1
-IFC_GRP_F_EXT=1
-IRQ_OUT=1
-TVDD_VSEL=0
-DVDD_VSEL=2
-EVDD_VSEL=2
-IIC2_EXT=1
-SYSCLK_FREQ=600
-HWA_CGA_M2_CLK_SEL=1
