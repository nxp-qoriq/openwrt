-.pbi
-// set CLK_SEL for QSPI CONFIG Register
-write 0x57015c, 0x20100000
-// set SCFG_SCRATCHRW1 ansd SCFG_SCRATCHRW2 for boot location ptr
-write 0x570600, 0x00000000
-write 0x570604, 0x40100000
